<AnchorPane fx:id="anchore" prefHeight="849.0" prefWidth="1320.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.yummyfoodfx.controller.AfficherCommandeView">
   <children>
      <BorderPane prefHeight="849.0" prefWidth="1320.0">
         <center>
            <AnchorPane fx:id="mainLayer" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ScrollPane layoutX="36.0" layoutY="109.0" prefHeight="588.0" prefWidth="893.0">
                     <content>
                        <GridPane fx:id="grid">
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                        </GridPane>
                     </content>
                  </ScrollPane>
                  <Button fx:id="btnPrete" layoutX="415.0" layoutY="715.0" mnemonicParsing="false" onAction="#makeCommandePrete" text="Prete" />
                  <Button fx:id="btnAnnuler" layoutX="563.0" layoutY="715.0" mnemonicParsing="false" onAction="#makeCommandeAnnuler" text="Annuler" />
                  <Label fx:id="idgetter" layoutX="401.0" layoutY="42.0" text="Label" />
                  <TextField fx:id="tfrecherche" layoutX="90.0" layoutY="59.0" promptText="Recherche" />
                  <ComboBox fx:id="cbtri" layoutX="642.0" layoutY="59.0" prefWidth="150.0" promptText="Tri" />
                  <Button layoutX="834.0" layoutY="59.0" mnemonicParsing="false" onAction="#tri" text="Tri" />
               </children>
            </AnchorPane>
         </center>
         <left>
            <VBox prefHeight="849.0" prefWidth="375.0" style="-fx-background-color: #C21A1ADD; -fx-alignment: TOP-CENTER;" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@logo.jpg" />
                     </image>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </ImageView>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
